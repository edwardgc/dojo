{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/MACHINE:X64",
                "-I${workspaceFolder}/../../googletest/googlemock/include",
                "-I${workspaceFolder}/../../googletest/googletest/include",
                "/Fd:",
                "${fileDirname}\\bin\\",
                "/Fo:",
                "${fileDirname}\\bin\\",
                "/Fp:",
                "${fileDirname}\\bin\\",
                "/Fe:",
                "${fileDirname}\\bin\\${fileBasenameNoExtension}.exe",
                "${workspaceFolder}\\..\\..\\googletest\\build\\lib\\Release\\**.lib",
                "${file}"
            ],
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build"
        },
        {
            "type": "shell",
            "label": "cmake build",
            "command": "cmake .",
            "args": [
            ],
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}